<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <PropertyGroup>
    <appname>bhl</appname>
    <TargetFrameworkVersion>v4.7.1</TargetFrameworkVersion>
    <DefineConstants>BHL_FRONT</DefineConstants>
    <SchemaVersion>2.0</SchemaVersion>
    <RootNamespace></RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="ANTLR">
      <HintPath>./deps/Antlr4.Runtime.Standard.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <Reference Include="opts">
      <HintPath>./deps/mono_opts.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <Compile Include="src/compile/*.cs" />
    <Compile Include="src/g/*.cs" />
    <Compile Include="src/vm/*.cs" />
    <Compile Include="src/std/*.cs" />
    <Compile Include="src/cmd/*.cs" />
    <Compile Include="src/lsp/*.cs" />
    <Compile Include="tests/*.cs" />
    <Compile Include="deps/msgpack/*.cs" />
    <Compile Include="deps/msgpack/Compiler/*.cs" />
  </ItemGroup>
  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
</Project>
